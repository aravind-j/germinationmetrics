@article{dahal_hydrothermal_1994,
	title = {Hydrothermal time analysis of tomato seed germination at suboptimal temperature and reduced water potential},
	volume = {4},
	issn = {0960-2585, 1475-2735},
	url = {https://www.cambridge.org/core/product/identifier/S096025850000204X/type/journal_article},
	doi = {10.1017/S096025850000204X},
	abstract = {Both temperature (\emph{T}) and water potential (ψ) have consistent and quantifiable effects on the rate and extent of seed germination (radicle emergence). Germination at suboptimal \emph{T} can be characterized on the basis of thermal time, or the \emph{T} in excess of a base (\emph{T$_{\textrm{b}}$}) multiplied by the time to a given percentage germination (\emph{t$_{\textrm{g}}$}). Similarly, germination at reduced ψ can be characterized on a hydrotime basis, or the ψ in excess of a base (ψ$_{\textrm{\emph{b}}}$) multiplied by \emph{t$_{\textrm{g}}$}. Within a seed population, the variation in thermal times to germination for a specific percentage (\emph{g}) is based upon the normal distribution of ψ$_{\textrm{\emph{b}}}$ values among seeds (ψ$_{\textrm{\emph{b}}}$(\emph{g})). Germination responses across a range of suboptimal \emph{T} and ψ might be accounted for by a general hydrothermal time model incorporating both \emph{T} and ψ components. We tested this hypothesis for tomato (\emph{Lycopersicon esculentum} Mill.) seeds of two genotypes differing in germination rates and tolerance of suboptimal \emph{T} and ψ. For combinations of \emph{T} (10−25°C) and ψ (0 to −0.9 MPa), a general hydrothermal time model accounted for approximately 75\% of the variation in times to germination within the seed populations of both genotypes, and over 96\% of the variation in median germination rates. However, ψ$_{\textrm{\emph{b}}}$(\emph{g}) distributions were sensitive to both the \emph{T} and ψ of imbibition, resulting in a poor fit of the model to specific time course data. Analysis of germination timing separately for low and high ψ ranges within a given \emph{T} resulted in specific models accounting for 88−99\% of the variation in individual germination times and \&gt;99\% of the variation in madian germination rates. Thus, for a given \emph{T} and ψ range, the hydrotime model closely matched tomato seed germination time courses. Accumulated hydrothermal time accounted well for germination rates at ψ\&gt; −0.5 MPa across suboptimal \emph{T} if ψ$_{\textrm{\emph{b}}}$(\emph{g}) was allowed to vary with \emph{T}. Germination did not show a consistent response to \emph{T} at ψ \&lt; −0.5 MPa, and estimated \emph{T}$_{\textrm{\emph{b}}}$ values varied over different \emph{T} ranges. Generalization of the hydrothermal time model across the entire range of suboptimal \emph{T} and ψ was limited by physiological adjustments of the seeds to their current environment. The hydrothermal time model detected and quantified these adjustment processes that would otherwise not be evident from inspection of germination time courses. Temperature and water potential influence the time to germination via physiological mechanisms that reciprocally interact.},
	language = {en},
	number = {2},
	urldate = {2021-05-18},
	journal = {Seed Science Research},
	author = {Dahal, Peetambar and Bradford, Kent J.},
	month = jun,
	year = {1994},
	pages = {71--80},
}

