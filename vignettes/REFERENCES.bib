@article{alvarado_hydrothermal_2005,
	title = {Hydrothermal time analysis of seed dormancy in true (botanical) potato seeds},
	volume = {15},
	issn = {0960-2585, 1475-2735},
	url = {https://www.cambridge.org/core/product/identifier/S0960258505000085/type/journal_article},
	doi = {10.1079/SSR2005198},
	abstract = {As seed dormancy is released within a seed population, both the rate and percentage of germination increase progressively with increasing dose of a dormancy-breaking treatment or condition. Population-based models can account for this behaviour on the basis of shifting response thresholds as dormancy is alleviated. In particular, hydrothermal time analysis of germination sensitivity to water potential (Ψ) and temperature ({<}span class="italic"{>}T{<}/span{>}) can describe these features of seed behaviour. We used the hydrothermal time model to analyse the effects of dormancy-breaking treatments on germination of dormant true (botanical) potato ({<}span class="italic"{>}Solanum tuberosum{<}/span{>} L.) seeds (TPS). After-ripening (37°C and 4\% seed moisture content) of TPS for 7 or 30 days partially or fully alleviated primary dormancy. The median base water potential required to prevent germination [Ψ{<}span class="sub"{>}b{<}/span{>}(50)] decreased from –0.25 MPa in control seeds to –0.87 MPa and –1.83 MPa after 7 and 30 days of after-ripening, respectively. In contrast, the base temperature for germination ({<}span class="italic"{>}T{<}/span{>}{<}span class="sub"{>}b{<}/span{>}) was relatively unaffected (0–3.3°C). Fluridone (50 μM), an inhibitor of abscisic acid (ABA) biosynthesis, also promoted germination of dormant TPS and lowered Ψ{<}span class="sub"{>}b{<}/span{>}(50), indicating a role for {<}span class="italic"{>}de novo{<}/span{>} synthesis of ABA during dormancy maintenance. Moist chilling (3 days at 4°C) or gibberellin (100 μM) alleviated secondary dormancy and lowered Ψ{<}span class="sub"{>}b{<}/span{>}(50) values from –0.08 MPa to –0.36 and –0.87 MPa, respectively. The hydrothermal time model allows quantification of dormancy levels and explains why changes in germination speed and percentage are closely correlated during dormancy alleviation.},
	language = {en},
	number = {2},
	urldate = {2021-05-18},
	journal = {Seed Science Research},
	author = {Alvarado, Veria and Bradford, Kent J.},
	month = jun,
	year = {2005},
	pages = {77--88},
}

