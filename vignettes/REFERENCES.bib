	volume = {47},
	issn = {0043-1745, 1550-2759},
	url = {https://www.cambridge.org/core/journals/weed-science/article/abs/modeling-germination-and-seedling-elongation-of-common-lambsquarters-chenopodium-album/8132553DF2F1A44A32F6D71B3ADF1835},
