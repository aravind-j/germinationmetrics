@article{negm_effects_1978,
	title = {Effects of ethylene and carbon dioxide on the germination of osmotically inhibited lettuce seed},
	volume = {62},
	copyright = {© 1978 American Society of Plant Biologists},
	issn = {0032-0889, 1532-2548},
	url = {http://www.plantphysiol.org/content/62/4/473},
	doi = {10.1104/pp.62.4.473},
	abstract = {Lettuce seeds ({<}em{>}Lactuca sativa{<}/em{>} L.) used in this study germinated 98\% at 25 C in light or dark. Their germination was completely inhibited by 0.20 {<}span class="small-caps"{>}m{<}/span{>} NaCl, 0.35 {<}span class="small-caps"{>}m{<}/span{>} mannitol, or polyethylene glycol 6000 (−7 bars) under continuous light when germination tests were made in Petri dishes. Approximately 50\% germination occurred in sealed flasks due to endogenously produced C$_{\textrm{2}}$H$_{\textrm{4}}$ and CO$_{\textrm{2}}$. Removal of either or both gases prevented germination. In the presence of endogenous CO$_{\textrm{2}}$, addition of C$_{\textrm{2}}$H$_{\textrm{4}}$ (0.5 to 16 microliters/liter) stimulated 95 to 100\% germination (after 5 days) only in the light, but the rate of germination was dependent on C$_{\textrm{2}}$H$_{\textrm{4}}$ concentration. At 16 microliters/liter C$_{\textrm{2}}$H$_{\textrm{4}}$, full germination occurred within 72 hours. Addition of up to 3.2\% CO$_{\textrm{2}}$ had no adverse effect on the C$_{\textrm{2}}$H$_{\textrm{4}}$ action. Higher concentrations or the complete absence of CO$_{\textrm{2}}$ reduced both rate and total germination. CO$_{\textrm{2}}$ alone was ineffective. Under these osmotic conditions the promotive effect of C$_{\textrm{2}}$H$_{\textrm{4}}$ was under the control of phytochrome.},
	language = {en},
	number = {4},
	urldate = {2020-01-24},
	journal = {Plant Physiology},
	author = {Negm, Fayek B. and Smith, Orrin E.},
	month = oct,
	year = {1978},
	pages = {473--476},
}

@article{lyon_rapid_1966,
	title = {Rapid method for determining numerical indexes for time-course curves},
	volume = {211},
	copyright = {1966 Nature Publishing Group},
	issn = {1476-4687},
	url = {https://www.nature.com/articles/211330a0},
	doi = {10.1038/211330a0},
	abstract = {IN reporting experiments with biological processes such as abscission, germination, and dormancy, where results are observed and recorded periodically, most investigators have found it convenient to present the data as time-course curves. While time-course curves such as those shown for abscission in Fig. 1 are useful in indicating the initiation and the total amount of abscission, as well as the slope of the response curves (rate), they cannot readily be compared numerically. For comparison of the results of large numbers of treatments in individual experiments, or of groups of experiments, and for statistical analyses, numerical designations are necessary. The time required for 50 or 100 per cent abscission and the percentage abscission at an arbitrary time after treatment are designations commonly used. Neither of these methods allows a satisfactory comparison of data frequently encountered in research. For example, comparison of the data represented by curves 1, 2, and 3 in Fig. 1 on the basis of the time to 50 or 100 per cent abscission is impossible as not all the curves reach either of these levels by the end of the experiment. This problem is intensified when treatments retard or inhibit abscission. Furthermore, the relationship of the curves changes with time, and thus comparison of the percentage abscission at an arbitrary time represents only a small part of the information in the curve.},
	language = {en},
	number = {5046},
	urldate = {2020-01-24},
	journal = {Nature},
	author = {Lyon, J. L. and Coffelt, R. J.},
	month = jul,
	year = {1966},
	pages = {330--330},
}

