@article{bould_model_1981,
	title = {A model for seed germination curves},
	abstract = {A mathematical model has been developed which describes the germination curve as a function of time. Unlike much of the previous work on the subject a recognisable growth equation or statistical distribution has not been used as the starting point. Instead it has been assumed that the simplest situation arises where each seed in a population has the same probability of germinating in unit time. More complex situations are modelled by assuming that the population comprises more than one sub-group where the probability of a seed germination in unit time is constant with a sub-group but varies between sub-groups. The shape of the curve is determined by the number of sub-groups and the extent to which they overlap. Starting with the basic equation for the simplest situation a range of curve shapes can be developed by building into the basic equation a second equation to describe the relationship between the probability (q) of a seed not germinating in unit time and time itself. Proofs are given for the shape of the curve for the basic equation and where there is a linear relationship between q and time and llog(, e) q and time. Examples are given of the model fitted to observed data.},
	language = {English},
	urldate = {2023-05-04},
	journal = {Seed Science and Technology},
	author = {Bould, A. and Abrol, B. K.},
	year = {1981},
}

